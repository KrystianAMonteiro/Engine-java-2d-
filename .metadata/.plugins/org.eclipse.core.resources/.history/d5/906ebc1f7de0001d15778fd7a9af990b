package jogo_pong1;

import java.awt.Color;
import java.awt.Graphics;

public class Enemy {
	
	public double x, y, width, height;
	
	public Enemy(int x, int y) {
		this.x = x;
		this.y =y;
		this.width = 40;
		this.height = 10;
	}
	
	
	public void tick() {
		
	}
	
	
	public void render(Graphics g) {
		g.setColor(Color.red);
		g.fillRect((int)x,(int) y, 40, 10);
	}

}
